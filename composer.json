{
    "name": "timetree/timetree-sdk-php",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.2",
        "firebase/php-jwt": "^5.2",
        "jawira/case-converter": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "TimeTreeWebApi\\": "src/TimeTreeWebApi"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "ulrichsg/getopt-php": "^3.4",
        "mockery/mockery": "^1.4",
        "overtrue/phplint": "^2.2"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\TimeTreeWebApi\\": "tests/TimeTreeWebApi"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit tests"
        ],
        "lint": [
            "vendor/bin/phplint"
        ]
    }
}
